Coord : {
  typing : { 
    type : "coord";
    %message : "I am me the type";
    %self : {() -> ^@;};
    %str : {() -> "(".con(^@.x.str().con(", ").con(^@.y.str()).con(")"));};
    %add : {(other) -> Coord(^@.x.add(other.x),^@.y.add(other.y));};
  };
  (x, y) -> {
    type : "coord";
    message : "Will be shown";
    x : x;
    y : y;
  };
};

a : Coord(10,20);
a.x.str().println();
a.y.str().println();
#a.str().println();

b : Coord(1,2);

#a.message.println();
#a.self().message.println();
#a.self().x.str().println();

