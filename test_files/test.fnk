!coord1 : {
  x : 10;
  y : 20;
};
!coord2 : {
  x : 30;
  y : 40;
};
!coord3 : {
  x : 50;
  y : 60;
};
relation : {
  c1 : coord1;
  c2 : coord2;
  () ?()-> {a : 5; b : 6;};
  () -> c2;
  (a, b) -> a.add(b);
  (a Boolean, b Boolean) -> a.eq(b);
  (a Boolean, b Boolean) ?(a) -> b;
};
a_coord : relation();