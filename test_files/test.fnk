!coord1 : {
  x : 10;
  y : 20;
};
!coord2 : {
  x : 30.0;
  y : 40.0;
};
!coord3 : {
  x : 50;
  y : 60;
};
!coord4 : {
  x : true;
  y : false;
};
!coord5 : {
  x : "Hello";
  y : "World";
};
relation : {
  c1 : coord1;
  c2 : coord2;
  () ?()-> {
    a : 5; 
    b : 6;
  };
  () -> c2;
  (a, b) -> a.add(b);
  (a bool, b bool) -> a.eq(b);
  (a bool, b bool) ?(a) -> b;
  -> c2;
};
a_coord : relation();
b_coord : a_coord.a;
c_coord : a_coord.b;